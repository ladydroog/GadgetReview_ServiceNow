        <description>Hide the Closed Skipped dropdown choice from all users except the Gadget Admins</description>
        <field>active</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Hide Closed Skipped</name>
        <order/>
        <script><![CDATA[function onLoad() {
   //Hide "Closed Skipped" state from everyone but gadget_admin
   if (g_user.hasRole('x_767916_gadget.gadget_admin'))
		return;

	if (g_form.getValue('incident_state') != '7')
		g_form.removeOption('incident_state', 7);
	if (g_form.getValue('state') != '7')
		g_form.removeOption('state', 7);
}]]></script>
